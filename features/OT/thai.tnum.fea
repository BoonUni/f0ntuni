  @TD.tnum = [ \uni0E50.tnum \uni0E51.tnum \uni0E52.tnum \uni0E53.tnum \uni0E54.tnum \uni0E55.tnum \uni0E56.tnum \uni0E57.tnum \uni0E58.tnum \uni0E59.tnum ];

  lookup Thai.tnum {
